<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="scores">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="score" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="studentCode" type="xs:string"/>
							<xs:element name="percentage" type="xs:int"/>
							<xs:element name="dateCompleted" type="xs:date"/>
							<xs:element name="timeSubmitted" type="xs:time"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>